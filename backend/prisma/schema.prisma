datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

model HealthCheck {
  id        Int      @id @default(autoincrement())
  status    String
  createdAt DateTime @default(now())
}
